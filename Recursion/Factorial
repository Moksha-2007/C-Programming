#include<stdio.h>
void main()
{
     int fact(int);
        int n, result;
           printf("Enter a Number: ");
           scanf("%d",&n);
           result=fact(n);
           printf("Factorial is %d",result);
}
 int fact(int n)
 {
    if(n==1)
    return 1;
     else
      return(n*fact(n-1));
 }
